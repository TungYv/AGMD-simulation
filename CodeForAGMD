# Comparasion on airgap of the model
airgap_Nvalues = np.array([2.0e-3,2.5e-3,3e-3,4e-3,5e-3]) # m
Jv_varies_airgap = np.zeros(len(airgap_Nvalues))
R_heat_r_airgap = np.zeros((6,len(airgap_Nvalues)))
R_heat_airgap = np.zeros((6,len(airgap_Nvalues)))
Nx = 20
for test_i,airgap_of_model in enumerate(airgap_Nvalues):
    print "The airgap of the model is: %.5f m" %airgap_of_model
    #compare_airgap = exp_condition(_vhi=.1,_vci=.1,_airgap=airgap_of_model,_length=1.,Nx=20)
    compare_airgap = AGMD(_L=1.,_W=1.,_H=2e-3,_delta_a=airgap_of_model,_delta_c=0.1e-3,_Kw=0.4*3.6,                    _d=.2e-6,_delta_m=100e-6,_tau=1.5,_mhi=11.76*60,_mci=8.94*60,_thi=80.,_tci=20.,_shi=42.,_sc=0.)
    compare_airgap.get_mesh_setup(Nx)
    compare_airgap.get_co_current(Nx);
    # relative resistance below
    R_heat_temp = compute_heat_resistance(compare_airgap)
    R_heat_r_airgap[0,test_i] = sum(R_heat_temp[1][0])/20
    R_heat_r_airgap[1,test_i] = sum(R_heat_temp[1][1])/20
    R_heat_r_airgap[2,test_i] = sum(R_heat_temp[1][2])/20
    R_heat_r_airgap[4,test_i] = sum(R_heat_temp[1][4])/20
    R_heat_r_airgap[5,test_i] = sum(R_heat_temp[1][5])/20
    # resistance below
    R_heat_temp = compute_heat_resistance(compare_airgap)
    R_heat_airgap[0,test_i] = sum(R_heat_temp[0][0])/20
    R_heat_airgap[1,test_i] = sum(R_heat_temp[0][1])/20
    R_heat_airgap[2,test_i] = sum(R_heat_temp[0][2])/20
    R_heat_airgap[4,test_i] = sum(R_heat_temp[0][4])/20
    R_heat_airgap[5,test_i] = sum(R_heat_temp[0][5])/20
    print
    print


# In[ ]:

T_list_labels = ["Thb","Thm",'Tma',"Tf","Tfp","Tpc","Tcb"]
T_list_values = [test_alsaadi.Thb,test_alsaadi.Thm,test_alsaadi.Tma,test_alsaadi.Tf,test_alsaadi.Tfp,test_alsaadi.Tpc,test_alsaadi.Tcb]
